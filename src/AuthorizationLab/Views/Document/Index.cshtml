@model System.Collections.Generic.IEnumerable<AuthorizationLab.Core.Models.Document>

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
    <div>
        <h1>Document Library</h1>
        @foreach (var doc in Model) {
            <p>
                @Html.ActionLink("Document #" + doc.Id, "Edit", new {id = doc.Id})
            </p>
        }
    </div>
</body>
</html>